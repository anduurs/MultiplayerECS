	<DataDef entityType="TileObject" xsi:type="FNEEntityData">
		<nameDef>rock_1_walkable</nameDef>
		<viewIsGameObject>false</viewIsGameObject>
		<pathingCost>0</pathingCost>
		<seeThrough>true</seeThrough>
		<blocking>false</blocking>
		<displayName>Small rock</displayName>
		<description>A fucking rock</description>
		<prefabPath>Prefabs/TileObject/Rock_1_walkable</prefabPath>
		<onDeathEffectPath>Prefabs/VFX/NatureDeath/RockDeath_VFX</onDeathEffectPath>
		<viewManager>EdgeObjectViewManager</viewManager>
		<editorCategory>Nature</editorCategory>
		<components>
			<IFNEComponentData xsi:type="SimpleInteractionComponentData">¨
				<hoverBoxDescription>
					<![CDATA[<color=#00ff00ff>[E]:</color> Pick up rocks]]>
				</hoverBoxDescription>
				<simpleInteractionModules>
					<InstantLootModule xsi:type="InstantLootModule">
						<itemToLootList>
							<itemToLootRef>rock</itemToLootRef>
						</itemToLootList>
						<itemMaximumAmount>3</itemMaximumAmount>
						<destroyObjectOnLoot>true</destroyObjectOnLoot>
					</InstantLootModule>
				</simpleInteractionModules>
			</IFNEComponentData>
		</components>
		<uiManager>SimpleInteractionUIManager</uiManager>
	</DataDef>

