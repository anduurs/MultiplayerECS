	<DataDef entityType="TileObject" xsi:type="FNEEntityData">
		<nameDef>rusty_pod</nameDef>
		<viewIsGameObject>false</viewIsGameObject>
		<pathingCost>250</pathingCost>
		<seeThrough>true</seeThrough>
		<displayName>Rusty Pod</displayName>
		<description>A lost container of some sort</description>
		<components>
			<IFNEComponentData xsi:type="MapMarkerComponentData">
				<iconName>Loot_Icon</iconName>
				<iconColor>#ff8000</iconColor>
				<iconScale>0.75</iconScale>
			</IFNEComponentData>
			<IFNEComponentData xsi:type="SimpleInteractionComponentData">¨
				<hoverBoxDescription>
					<![CDATA[<color=#ff0000ff>Warning:</color> The pod is still secured!]]>
				</hoverBoxDescription>
				<simpleInteractionModules>
					<TriggerEventModule xsi:type="TriggerEventModule">
						<eventRef>event_tiny_scatter_1</eventRef>
					</TriggerEventModule>
					<PlayEffectModule xsi:type="PlayEffectModule">
						<effectRef>nail_ring_burst</effectRef>
					</PlayEffectModule>
					<PlayEffectModule xsi:type="PlayEffectModule">
						<effectRef>effect_alarm</effectRef>
					</PlayEffectModule>
					<ReplaceStaticEntityModule xsi:type="ReplaceStaticEntityModule">
						<objectRef>rusty_pod_opened</objectRef>
					</ReplaceStaticEntityModule>
					<CreateNoiseModule xsi:type="CreateNoiseModule">
						<noiseRange>50</noiseRange>
					</CreateNoiseModule>
				</simpleInteractionModules>
			</IFNEComponentData>
		</components>
		<uiManager>SimpleInteractionUIManager</uiManager>
		<prefabPath>Prefabs/TileObject/Crashed_Pod</prefabPath>
		<viewManager>EdgeObjectViewManager</viewManager>
	</DataDef>

